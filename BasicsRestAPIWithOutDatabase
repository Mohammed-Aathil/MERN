const express = require("express");
const app = express();
const PORT = 3000;



app.get('/todos', (req, res) => {
    res.json([
        {
            task : "Remove Mohammed Aathil J",
            priority : "High"
        },
    ]);
});



app.listen(PORT ,() => {
    console.log(`Server running at a port ${PORT}`)
});
